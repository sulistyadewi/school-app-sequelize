<%- include('partial/head2.ejs') %>
  <body>
    <h1>Teacher</h1>
    <table>
      <th>
        <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Mail</th>
          <th>Gender</th>
          <th class="action"></th>
        </tr>
      </th>
      <tbody>
        <% for (let i = 0; i < data.length; i++ ) { %>
        <tr>
          <td><%= data[i].id %></td>
          <td><%= data[i].first_name %></td>
          <td><%= data[i].last_name %></td>
          <td><%= data[i].email %></td>
          <td><%= data[i].gender %></td>
          <td>
            <a href="#" class="edit">Edit</a>
            <span>|</span>
            <a href="#" class="remove">Remove</a>
          </td>
        </tr>
        <% } %>
      </tbody>
    </table>
    <button><a href="/teacher/add">add</a></button>
  </body>
</html>
